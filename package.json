{
  "name": "airkeeper",
  "license": "MIT",
  "version": "1.0.0",
  "description": "A tool to update a beacon server value on a time interval",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "sls:config": "sls config credentials --provider aws --key $AWS_ACCESS_KEY_ID --secret $AWS_SECRET_ACCESS_KEY",
    "sls:deploy": "sls deploy --region $REGION --stage $STAGE",
    "sls:remove": "sls remove --region $REGION --stage $STAGE",
    "sls:invoke": "sls invoke --function updateBeacon",
    "sls:invoke-local": "tsc && sls invoke local --function updateBeacon"
  },
  "dependencies": {
    "@api3/airnode-adapter": "^0.2.2",
    "@api3/airnode-node": "^0.2.2",
    "@api3/airnode-protocol": "^0.2.2",
    "ethers": "^5.5.2",
    "lodash": "^4.17.21",
    "serverless": "^2.67.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "rimraf": "^3.0.2"
  }
}
